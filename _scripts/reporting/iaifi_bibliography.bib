author = {Mercado, Francisco J. and Moreno, Jorge and Feldmann, Robert and Zeender, Marckie and Benavides, Jos√© A. and Piotrowska, Joanna M. and Klein, Courtney and Wheeler, Coral and Necib, Lina and Bullock, James S. and Hopkins, Philip F.},
title = {Effects of Galactic Environment on Size and Dark Matter Content in Low-mass Galaxies},
journal = {The Astrophysical Journal},
abstract = {We utilize the cosmological volume simulation FIREbox to investigate how a galaxy‚Äôs environment influences its size and dark matter content. Our study focuses on approximately 1200 galaxies (886 central and 332 satellite halos) in the low-mass regime, with stellar masses between 106 and 109M‚äô. We analyze the size‚Äìmass relation (r50‚ÄìM‚ãÜ), the inner dark matter mass‚Äìstellar mass (‚ÄìM‚ãÜ) relation, and the halo mass‚Äìstellar mass (Mhalo‚ÄìM‚ãÜ) relation. At fixed stellar mass, we find that galaxies experiencing stronger tidal influences, indicated by higher Perturbation Indices (PI &gt; 1) are generally larger and have lower halo masses relative to their counterparts with lower Perturbation Indices (PI &lt; 1). Applying a Random Forest regression model, we show that both the environment (PI) and halo mass (Mhalo) are significant predictors of a galaxy‚Äôs relative size and dark matter content. Notably, because Mhalo is also strongly affected by the environment, our findings indicate that environmental conditions not only influence galactic sizes and relative inner dark matter content directly, but also indirectly, through their impact on halo mass. Our results highlight a critical interplay between environmental factors and halo mass in shaping galaxy properties, affirming the environment as a fundamental driver in galaxy formation and evolution.},
eprint={2501.04084},
archivePrefix={arXiv},
}

